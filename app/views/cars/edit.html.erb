<%= form_for(@car) do |f| %>
  <%= f.label :brand %>
  <%= f.text_field :brand %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :engine %>
  <%= f.text_field :engine %>
  <%= f.label :color %>
  <%= f.text_field :color %>
  <%= f.label :car_type %>
  <%= f.text_field :car_type %>
  <%= f.submit %>
<% end %>
